    <div class="cards-container flex justify-between space-x-6">
      <!-- Card 1 -->
      <div class="card">
        <div class="pricing-block-content">
          <p class="pricing-plan">Car Washing</p>
          <div class="price-value">
            <p class="price-number">DT/<span class="price-integer">30</span></p>
          </div>
          <div class="pricing-note">INTERIOR</div>
          <ul class="check-list">
            <li class="check-list-item">EXTERIOR</li>
          </ul>
        </div>
      </div>

      <!-- Card 2 -->
