        <div class="pricing-block-content">
          <p class="pricing-plan">Oil Change</p>
          <div class="price-value">
            <p class="price-number">DT/<span class="price-integer">25</span></p>
            <div id="priceDiscountCent"></div>
          </div>
          <div class="pricing-note">Note!</div>
          <ul class="check-list">
            <li class="check-list-item">oil not included you can buy your own oil</li>

          </ul>
        </div>
      </div>

      <!-- Card 3 -->
      <div class="card">
        <div class="pricing-block-content">
          <p class="pricing-plan">Ceramic protection</p>
          <div class="price-value">
            <p class="price-number">DT/<span class="price-integer">150</span></p>
            <div id="priceDiscountCent"></div>
          </div>
          <div class="pricing-note"></div>
          <ul class="check-list">
            <li class="check-list-item">make your car shiny again</li>
         
          </ul>
        </div>
      </div>

      <!-- Card 4 -->
      <div class="card">
        <div class="pricing-block-content">
          <p class="pricing-plan">Engine Fixes</p>
          <div class="price-value">
            <p class="price-number">Custom</p>
            <div id="priceDiscountCent">Pricing</div>
          </div>
          <div class="pricing-note"></div>
          <ul class="check-list">
            <li class="check-list-item">Price after our fixes</li>
          </ul>
        </div>
      </div>
    </div>
  </div>
